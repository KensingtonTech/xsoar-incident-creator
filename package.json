    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "^9.1.1",
    "@gorillastack/jmespath": "^1.3.0",
    "dayjs": "^1.8.27",
    "jsencrypt": "^3.0.0-rc.1",
    "ng-click-outside": "^6.0.0",
