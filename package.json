{
  "name": "xsoar-incident-creator",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build-dev",
    "build-dev": "ng build",
    "build-prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server": "cd server/src && node server.js",
    "server-dev": "cd server/src && node server.js --dev",
    "build-docker-dev": "npm run build-dev && docker build --no-cache=true -t xsoar-incident-creator:dev .",
    "save-docker-dev": "docker save xsoar-incident-creator:dev | gzip > xic-dev-docker-image.tgz",
    "fixdeps": "cp -r node_modules/ace-builds/src-min/*-json.js src/assets",
    "postinstall": "npm run fixdeps"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.11",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "~12.2.11",
    "@angular/compiler": "~12.2.11",
    "@angular/core": "~12.2.11",
    "@angular/forms": "~12.2.11",
    "@angular/platform-browser": "~12.2.11",
    "@angular/platform-browser-dynamic": "~12.2.11",
    "@angular/router": "^12.2.11",
    "@gorillastack/jmespath": "^1.3.0",
    "ace-builds": "1.4.13",
    "dayjs": "^1.8.27",
    "jsencrypt": "^3.0.0-rc.1",
    "ng-click-outside": "^6.0.0",
    "ng2-ace-editor": "^0.3.9",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.6",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.11",
    "@angular/cli": "~12.2.11",
    "@angular/compiler-cli": "~12.2.11",
    "@angular/language-service": "~12.2.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.6",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "^8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}